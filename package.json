{
	"name": "modulscript",
	"version": "0.0.3",
	"publisher": "kajdev",
	"author": {
		"name": "kaj"
	},
	"license": "MIT",
	"icon": "icon.png",
	"description": "ModulScript language support for Visual Studio Code",
	"displayName": "ModulScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/kajdev/vscode-modulscript.git"
	},
	"bugs": {
		"url": "https://github.com/kajdev/vscode-modulscript/issues"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Linters"
	],
	"engines": {
		"vscode": "^1.0.0"

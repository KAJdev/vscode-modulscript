	"contributes": {
		"languages": [
			{
				"id": "modulscript",
				"aliases": [
					"ModulScript"
				],
                "extensions": [".ms"],
				"configuration": "./modulscript.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "modulscript",
				"scopeName": "source.modulscript",
				"path": "./syntaxes/modulscript.json"
			}
		]
	}
